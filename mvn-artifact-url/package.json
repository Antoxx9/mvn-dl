{
  "name": "mvn-artifact-url",
  "version": "2.0.1",
  "description": "Create url for maven artifacts",
  "main": "built/artifact-url.js",
  "typings": "built/artifact-url.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json src/*",
    "test:unit": "mocha --compilers js:babel-register --reporter=tap",
    "test:readme": "readme-assert --require ./test/readme.setup.js",
    "test": "npm run test:unit && npm run test:readme",
    "build": "tsc",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laat/mvn-dl.git"
  },
  "keywords": [
    "maven",
    "artifact"
  ],
  "author": "Sigurd Fosseng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laat/mvn-dl/issues"
  },
  "homepage": "https://github.com/laat/mvn-dl#readme",
  "dependencies": {
    "mvn-artifact-filename": "^2.0.0",
    "xml2js" : "^0.4.17",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "*",
    "chai-as-promised": "^5.1.0",
    "mocha": "*",
    "mock-fs": "^3.5.0",
    "nock": "^8.0.0",
    "onchange": "^2.2.0",
    "readme-assert": "^2.1.2",
    "tslint": "^3.7.1",
    "typescript": "^1.8.9"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
