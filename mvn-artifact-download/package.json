{
  "name": "mvn-artifact-download",
  "version": "2.0.1",
  "description": "Download maven artifacts",
  "main": "built/artifact-download.js",
  "typings": "built/artifact-download.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json src/*",
    "test": "npm run lint && mocha --compilers js:babel-register",
    "build": "tsc --project ./tsconfig.json",
    "prepublish": "typings -- install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laat/mvn-dl.git"
  },
  "keywords": [
    "maven",
    "artifact"
  ],
  "author": "Sigurd Fosseng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laat/mvn-dl/issues"
  },
  "homepage": "https://github.com/laat/mvn-dl#readme",
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "*",
    "chai-as-promised": "^6.0.0",
    "mocha": "*",
    "mock-fs": "^4.3.0",
    "nock": "^9.0.13",
    "onchange": "^3.2.1",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2",
    "typings": "^2.1.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "mvn-artifact-name-parser": "~2.0.0",
    "mvn-artifact-filename": "~2.0.0",
    "mvn-artifact-url": "~2.0.0",
    "request": "^2.67.0"
  }
}
