{
  "name": "mvn-artifact-download",
  "version": "1.0.1",
  "description": "Download maven artifacts",
  "main": "dist/artifact-download.js",
  "jsnext:main": "lib/artifact-download.js",
  "scripts": {
    "build": "babel lib -d dist",
    "post-build": "onchange 'dist/**.js' 'test/**.js' -- npm test",
    "watch": "npm run build -- -w & npm run post-build",
    "test": "standard && mocha --compilers js:babel-register",
    "prepublish": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laat/mvn-dl.git"
  },
  "keywords": [
    "maven",
    "artifact"
  ],
  "author": "Sigurd Fosseng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laat/mvn-dl/issues"
  },
  "homepage": "https://github.com/laat/mvn-dl#readme",
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "*",
    "chai-as-promised": "^5.1.0",
    "mocha": "*",
    "mock-fs": "^3.5.0",
    "nock": "^3.5.0",
    "onchange": "1.x",
    "standard": "*"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "standard": {
    "ignore": [
      "coverage/",
      "dist/",
      "node_modules/"
    ]
  },
  "dependencies": {
    "mvn-artifact-name-parser": "~1.0.0",
    "mvn-artifact-filename": "~1.0.0",
    "mvn-artifact-url": "~1.0.0",
    "es6-promise": "^3.0.2",
    "request": "^2.67.0"
  }
}
