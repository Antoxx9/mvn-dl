{
  "name": "mvn-artifact-filename",
  "version": "1.0.3",
  "description": "create filename for maven artifact",
  "main": "./built/filename.js",
  "typings": "./built/filename.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json src/*",
    "test:unit": "mocha --compilers js:babel-register --reporter=tap",
    "test:readme": "readme-assert",
    "test": "npm run test:readme && npm run test:unit",
    "build": "tsc",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laat/mvn-dl.git"
  },
  "keywords": [
    "maven",
    "artifact"
  ],
  "author": "Sigurd Fosseng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laat/mvn-dl/issues"
  },
  "homepage": "https://github.com/laat/mvn-dl#readme",
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "*",
    "mocha": "*",
    "onchange": "1.x",
    "readme-assert": "^1.0.0",
    "tslint": "next",
    "typescript": "next"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
